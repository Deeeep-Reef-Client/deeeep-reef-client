{
    "manifest_version": 2,
    "name": "Deeeep.io Reef Client Asset Swapper",
    "description": "DRC's asset swapper. Original code by Doctorpus (doc-assets)",
    "version": "1.0.0",
    "background": {"scripts":["background.js"]},
    "browser_action": {
        "default_title": "idk"
    }, 
  "permissions": [
    "webRequest",
    "https://deeeep.io/*",
    "https://mapmaker.deeeep.io/*",
    "https://beta.deeeep.io/*",
    "https://cdn.deeeep.io/*",
    "https://api.deeeep.io/*",
    "https://apibeta.deeeep.io/*",
    "webRequestBlocking",
    "storage"
  ]
}